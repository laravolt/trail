{
    "name": "laravolt/trail",
    "description": "Just another audit trail for Eloquent",
    "keywords": [
        "laravel",
        "laravolt",
        "trail",
        "audit",
        "log",
        "revision"
    ],
    "homepage": "https://github.com/laravolt/trail",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Trail\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Trail\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
